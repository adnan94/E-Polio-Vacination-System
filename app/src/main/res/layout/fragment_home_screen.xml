<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:background="#fff"
    tools:context="com.example.ali.myapplication.Activities.UI.UserHomeFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
      <com.example.ali.myapplication.Activities.Utils.ViewPagerCustomDuration
          android:id="@+id/viewPager"
          android:layout_width="match_parent"
          android:layout_height="180dp"
          android:layout_centerInParent="true">

      </com.example.ali.myapplication.Activities.Utils.ViewPagerCustomDuration>

        <Button
            android:id="@+id/form_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Form" />

        <Button
            android:id="@+id/form_btn_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/form_btn"
            android:text="List of forms" />
    </LinearLayout>
</FrameLayout>
